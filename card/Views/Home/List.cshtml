@model IEnumerable<card.Models.Truyen>
@{
    Layout = null;
}
<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="~/css/truyen.css?i=1" type="text/css">
</head>
<body>
    @foreach (var i in Model)
    {
        <div class="card">
            <div class="card__img">
                <img src="@i.imgUrl" alt="trinh duyet khong ho tro">
            </div>
            <div class="card__title">
                <p>@i.ten</p>
            </div>
            <div class="card__details">
                <a href="~/Home/Chitietsp/@i.id">chi tiet san pham, >>></a>
            </div>
        </div>
    }
    
</body>
</html>